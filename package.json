{
  "name": "express-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-cjs && npm run build-es6",
    "build-cjs": "rimraf lib && babel ./modules -d lib --ignore '__tests__'",
    "build-es6": "rimraf es6 && babel ./modules -d es6 --blacklist=es6.modules --ignore '__tests__'",
    "build-umd": "NODE_ENV=production webpack modules/index.js umd/ReactRouter.js",
    "build-min": "NODE_ENV=production webpack -p modules/index.js umd/ReactRouter.min.js",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build/",
    "deploy": "rm -rf dist; NODE_ENV=production webpack -p"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.3",
    "history": "^1.17.0",
    "jade": "^1.11.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-plugin-dev-expression": "^0.1.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.0",
    "express-urlrewrite": "^1.2.0",
    "file-loader": "^0.8.5",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0"
  }
}
